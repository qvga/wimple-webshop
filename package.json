        "lodash": "^4.17.4",
        "rxjs": "^6.2.2",
        "style-loader": "^0.22.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue": "^2.5.7",
        "vue-router": "^3.0.1",
        "vue-rx": "^6.0.1",
